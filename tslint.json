{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "quotemark": [true, "single", "avoid-escape"],
      "semicolon": [true, "never"],
      "no-unused-expression": [true, "allow-new"],
      "no-consecutive-blank-lines": [true, 2]
    },
    "rulesDirectory": []
}
