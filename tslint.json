{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "quotemark": [true, "single", "avoid-escape"],
      "semicolon": [true, "never"],
      "no-unused-expression": [true, "allow-new"],
      "no-consecutive-blank-lines": [true, 2],
      "ter-indent": [true, 4],
      "variable-name": [true, "allow-snake-case", "check-format", "allow-leading-underscore"],
      "object-literal-key-quotes": [true, "consistent"],
      "no-namespace": false
    },
    "rulesDirectory": []
}
